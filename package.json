{
  "name": "@rsgarate/rut",
  "version": "0.1.2",
  "description": "Modern JavaScript Rut utility library",
  "license": "MIT",
  "author": "'Ramses Garate <'rsgarate@gmail.com'>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "keywords": [
    "rut",
    "dni",
    "chile"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramsesgarate/rutjs.git"
  },
  "bugs": {
    "url": "https://github.com/ramsesgarate/rutjs/issues"
  },
  "homepage": "https://github.com/ramsesgarate/rutjs#readme",
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --ext .ts",
    "prepack": "unbuild",
    "release": "pnpm test && standard-version && git push --follow-tags && pnpm publish --access=public",
    "test": "pnpm lint && vitest run --coverage"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "@vitest/coverage-c8": "0.22.1",
    "eslint": "8.23.0",
    "standard-version": "9.5.0",
    "typescript": "4.8.2",
    "unbuild": "0.8.9",
    "vite": "^3.1.0",
    "vitest": "0.22.1"
  }
}
