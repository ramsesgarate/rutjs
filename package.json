{
  "name": "@dnijs/rut",
  "version": "0.1.1",
  "description": "Modern JavaScript Rut utility library",
  "license": "MIT",
  "author": "Ramses Garate",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
